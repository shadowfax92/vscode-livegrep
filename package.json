{
  "name": "livegrep",
  "displayName": "Live Grep",
  "description": "Ripgrep in the command palette",
  "license": "SEE LICENSE IN LICENSE",
  "publisher": "YomiTosh",
  "repository": {
    "type": "git",
    "url": "https://github.com/abayomi185/vscode-livegrep"
  },
  "icon": "docs/icon.png",
  "galleryBanner": {
    "color": "#4D3C77",
    "theme": "dark"
  },
  "author": {
    "name": "Yomi"
  },
  "homepage": "https://github.com/abayomi185/vscode-livegrep/blob/main/README.md",
  "keywords": [
    "ripgrep",
    "rg",
    "livegrep",
    "grep",
    "search",
    "find",
    "fzf",
    "fuzzy",
    "fuzzy search",
    "fuzzy find",
    "fuzzy grep"
  ],
  "pricing": "Free",
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "livegrep.search",
        "title": "LiveGrep: Search"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/shell-quote": "^1.7.1",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@vscode/ripgrep": "^1.15.5",
    "shell-quote": "^1.8.1"
  }
}
